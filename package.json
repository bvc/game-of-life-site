{
  "name": "game-of-life-site",
  "version": "0.0.1",
  "description": "Conway's game of life",
  "author": "bvc",
  "license": "MIT",
  "scripts": {
    "start": "babel-node tools/src-server.js",
    "lint": "esw webpack.config.* src tools --color",
    "lint:watch": "npm run lint -- --watch",
    "remove-dist": "rimraf ./dist",
    "clean-dist": "npm run remove-dist && mkdir dist",
    "prebuild": "npm run clean-dist",
    "build": "babel-node tools/build-prod.js && npm run open:dist",
    "open:src": "babel-node tools/src-server.js",
    "open:dist": "babel-node tools/dist-server.js",
    "test": "mocha bin/test-setup.js \"src/**/*.spec.js\" --reporter progress"
  },
  "dependencies": {
    "bootstrap": "3.3.7",
    "jquery": "3.2.1"
  },
  "devDependencies": {
    "ajv": "6.5.1",
    "autoprefixer": "8.6.3",
    "babel-cli": "6.26.0",
    "babel-core": "6.26.3",
    "babel-eslint": "8.2.5",
    "babel-loader": "7.1.4",
    "babel-polyfill": "6.26.0",
    "babel-preset-env": "1.7.0",
    "babel-preset-stage-1": "6.24.1",
    "browser-sync": "2.24.4",
    "chai": "4.1.2",
    "connect-history-api-fallback": "1.5.0",
    "css-loader": "0.28.11",
    "eslint": "4.19.1",
    "eslint-plugin-import": "2.12.0",
    "eslint-watch": "3.1.5",
    "file-loader": "1.1.11",
    "html-webpack-plugin": "3.2.0",
    "mini-css-extract-plugin": "0.4.0",
    "mocha": "5.2.0",
    "node-sass": "4.9.0",
    "postcss-loader": "2.1.5",
    "rimraf": "2.6.2",
    "sass-loader": "7.0.3",
    "sinon": "6.0.0",
    "sinon-chai": "3.2.0",
    "style-loader": "0.21.0",
    "url-loader": "1.0.1",
    "webpack": "4.12.0",
    "webpack-dev-middleware": "3.1.3",
    "webpack-hot-middleware": "2.22.2",
    "webpack-md5-hash": "0.0.6"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bvc/game-of-life-site"
  }
}
