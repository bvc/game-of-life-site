{
  "name": "game-of-life-site",
  "version": "0.0.1",
  "description": "Conway's game of life",
  "scripts": {
    "start": "babel-node bin/dev-server.js",
    "lint": "esw webpack.config.* src bin --color",
    "lint:watch": "npm run lint -- --watch",
    "clean-dist": "npm run remove-dist && mkdir dist",
    "remove-dist": "rimraf ./dist",
    "open:prod": "babel-node bin/prod-server.js",
    "prebuild": "npm run clean-dist",
    "build": "babel-node bin/build.js && npm run open:prod",
    "test": "mocha bin/test-setup.js \"src/**/*.spec.js\" --reporter progress"
  },
  "author": "bvc",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "6.11.4",
    "babel-core": "6.11.4",
    "babel-loader": "6.2.4",
    "babel-preset-es2015": "6.9.0",
    "babel-register": "6.9.0",
    "browser-sync": "2.13.0",
    "chai": "3.5.0",
    "css-loader": "0.23.1",
    "eslint": "3.1.1",
    "eslint-plugin-import": "1.11.0",
    "eslint-watch": "2.1.13",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.9.0",
    "html-webpack-plugin": "2.22.0",
    "mocha": "2.5.3",
    "node-sass": "3.8.0",
    "npm-run-all": "2.3.0",
    "rimraf": "2.5.3",
    "sass-loader": "4.0.0",
    "sinon": "1.17.4",
    "sinon-chai": "2.8.0",
    "style-loader": "0.13.1",
    "url-loader": "0.5.7",
    "webpack": "1.13.1",
    "webpack-dev-middleware": "1.6.1",
    "webpack-hot-middleware": "2.12.1",
    "webpack-md5-hash": "0.0.5"
  },
  "dependencies": {
    "bootstrap": "3.3.6",
    "jquery": "3.1.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bvc/game-of-life-site"
  }
}
