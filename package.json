{
  "name": "game-of-life-site",
  "version": "0.0.1",
  "description": "Conway's game of life",
  "scripts": {
    "start": "babel-node bin/dev-server.js",
    "lint": "esw webpack.config.* src bin --color",
    "lint:watch": "npm run lint -- --watch",
    "clean-dist": "npm run remove-dist && mkdir dist",
    "remove-dist": "rimraf ./dist",
    "open:prod": "babel-node bin/prod-server.js",
    "prebuild": "npm run clean-dist",
    "build": "babel-node bin/build.js && npm run open:prod",
    "test": "mocha bin/test-setup.js \"src/**/*.spec.js\" --reporter progress"
  },
  "author": "bvc",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "6.24.1",
    "babel-core": "6.25.0",
    "babel-loader": "7.0.0",
    "babel-preset-es2015": "6.24.1",
    "babel-register": "6.24.1",
    "browser-sync": "2.18.12",
    "chai": "4.0.2",
    "css-loader": "0.28.4",
    "eslint": "3.19.0",
    "eslint-plugin-import": "2.3.0",
    "eslint-watch": "3.1.1",
    "extract-text-webpack-plugin": "2.1.2",
    "file-loader": "0.11.2",
    "html-webpack-plugin": "2.28.0",
    "mocha": "3.4.2",
    "node-sass": "4.5.3",
    "npm-run-all": "4.0.2",
    "rimraf": "2.6.1",
    "sass-loader": "6.0.6",
    "sinon": "2.3.4",
    "sinon-chai": "2.11.0",
    "style-loader": "0.18.2",
    "url-loader": "0.5.9",
    "webpack": "1.14.0",
    "webpack-dev-middleware": "1.10.2",
    "webpack-hot-middleware": "2.18.0",
    "webpack-md5-hash": "0.0.5"
  },
  "dependencies": {
    "bootstrap": "3.3.7",
    "jquery": "3.2.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bvc/game-of-life-site"
  }
}
