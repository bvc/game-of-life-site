{
  "name": "game-of-life-site",
  "version": "0.0.1",
  "description": "Conway's game of life",
  "scripts": {
    "start": "babel-node bin/dev-server.js",
    "lint": "esw webpack.config.* src bin --color",
    "lint:watch": "npm run lint -- --watch",
    "clean-dist": "npm run remove-dist && mkdir dist",
    "remove-dist": "rimraf ./dist",
    "open:prod": "babel-node bin/prod-server.js",
    "prebuild": "npm run clean-dist",
    "build": "babel-node bin/build.js && npm run open:prod",
    "test": "mocha bin/test-setup.js \"src/**/*.spec.js\" --reporter progress"
  },
  "author": "bvc",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "6.23.0",
    "babel-core": "6.23.1",
    "babel-loader": "6.3.2",
    "babel-preset-es2015": "6.22.0",
    "babel-register": "6.23.0",
    "browser-sync": "2.18.8",
    "chai": "3.5.0",
    "css-loader": "0.26.1",
    "eslint": "3.16.1",
    "eslint-plugin-import": "2.2.0",
    "eslint-watch": "3.0.0",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.10.0",
    "html-webpack-plugin": "2.28.0",
    "mocha": "3.2.0",
    "node-sass": "4.5.0",
    "npm-run-all": "4.0.2",
    "rimraf": "2.6.1",
    "sass-loader": "6.0.2",
    "sinon": "1.17.7",
    "sinon-chai": "2.8.0",
    "style-loader": "0.13.1",
    "url-loader": "0.5.7",
    "webpack": "1.14.0",
    "webpack-dev-middleware": "1.10.1",
    "webpack-hot-middleware": "2.17.0",
    "webpack-md5-hash": "0.0.5"
  },
  "dependencies": {
    "bootstrap": "3.3.7",
    "jquery": "3.1.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bvc/game-of-life-site"
  }
}
